{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/display-name": 1

  },
   "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": false
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
      "classes": true,
      "jsx": true
  },
  "plugins": [
    "react"
  ],
}
